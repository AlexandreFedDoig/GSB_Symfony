    <h1>RapportVisite</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>VisMatricule</th>
                <td>{{ rapport_visite.visMatricule }}</td>
            </tr>
            <tr>
                <th>RapNum</th>
                <td>{{ rapport_visite.rapNum }}</td>
            </tr>
            <tr>
                <th>PraNum</th>
                <td>{{ rapport_visite.praNum }}</td>
            </tr>
