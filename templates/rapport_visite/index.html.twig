{% extends 'base.html.twig' %}

{% block title %}RapportVisite index{% endblock %}

{% block body %}
    <h1>RapportVisite index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>VisMatricule</th>
                <th>RapNum</th>
                <th>PraNum</th>
                <th>RapDate</th>
                <th>RapBilan</th>
                <th>RapMotif</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for rapport_visite in rapport_visites %}
            <tr>
                <td>{{ rapport_visite.visMatricule }}</td>
                <td>{{ rapport_visite.rapNum }}</td>
                <td>{{ rapport_visite.praNum }}</td>
                <td>{{ rapport_visite.rapDate ? rapport_visite.rapDate|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ rapport_visite.rapBilan }}</td>
                <td>{{ rapport_visite.rapMotif }}</td>
                <td>
                    <a href="{{ path('rapport_visite_show', {'visMatricule': rapport_visite.visMatricule}) }}">show</a>
                    <a href="{{ path('rapport_visite_edit', {'visMatricule': rapport_visite.visMatricule}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('rapport_visite_new') }}">Create new</a>
{% endblock %}
