<form method="post" action="{{ path('labo_delete', {'labCode': labo.labCode}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ labo.labCode) }}">
    <button class="btn">Delete</button>
</form>
